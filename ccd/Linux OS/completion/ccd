if [ -r "/tmp/ccd.txt" ]; then
    mapfile -t _all_dir_names < <(cat /tmp/ccd.txt)
else
    source ~/.bashrc
    saveDirs
    mapfile -t _all_dir_names < <(cat /tmp/ccd.txt)
fi
_ccd(){
    local dir cur=${COMP_WORDS[COMP_CWORD]}
    COMPREPLY=()
    eval cur="$cur"
    for dir in "${_all_dir_names[@]}"; do
        if [[ $dir == "$cur"* ]]; then
            printf -v dir "%q" "$dir"
            COMPREPLY+=("$dir")
        fi
    done
}
complete -F _ccd ccd
